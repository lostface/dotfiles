  "launch": {
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Webpack Dev Server",
        "program": "${workspaceFolder}/build/index.js",
        "args": ["--dev-server"],
        "envFile": "${workspaceFolder}/.env",
        "env": { "NODE_ENV": "development" }
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Webpack Prod Server",
        "program": "${workspaceFolder}/build/index.js",
        "args": ["--dev-server"],
        "envFile": "${workspaceFolder}/.env",
        "env": { "NODE_ENV": "production" }
